<p id="notice"><%= notice %></p>

<h1>Clubs</h1>
<%= link_to 'New Club', new_club_path, class:'btn btn-secondary', remote: true %>


<table class='table'>
  <thead>
    <tr>
      <th>Name</th>
      <th>Country</th>
      <th>Trophies</th>
      <th colspan="3">Action</th>
    </tr>
  </thead>

  <tbody id='clubTable'>
    <% @clubs.each do |club| %>
      <%= render 'club_row', club: club %>
    <% end %>
  </tbody>
</table>

